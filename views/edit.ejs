    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    
    <!-- Datepicker JavaScript -->
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script>
        $( function() {
        $( "#dob" ).datepicker({dateFormat: "mm-dd-yy", changeMonth: true,
            changeYear: true, yearRange: '1900:2021'});
        } );
    </script>

</head>
<body>
    <div class="container my-5">
        <div class="row">
            <div class="col-sm-6 mx-auto">
            
                <form method="POST" action="/editconfirm">
                    <div class="mb-3">
                        <label for="name" class="form-label">Name</label>
                        <input type="text" class="form-control" name="name" id="name" value="<%= name %>" oninput="let p=this.selectionStart;this.value=this.value.toUpperCase();this.setSelectionRange(p, p);" required>
                    </div>

                    <input type="hidden" name="formMethod" value="singup">
                    <button type="submit" class="btn btn-info mt-3">Save</button>

                </form>
            </div>
        </div>
    </div>
</body>

<!-- Input Mask JavaScript -->
